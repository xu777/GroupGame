<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="groupgame.MainController">
    <children>
      <AnchorPane layoutX="90.0" layoutY="160.0" prefHeight="480.0" prefWidth="270.0" styleClass="whitepanes" stylesheets="@style.css">
         <children>
            <Button layoutX="89.0" layoutY="150.0" mnemonicParsing="false" onAction="#newGameClick" prefHeight="25.0" prefWidth="93.0" text="New" />
            <Button layoutX="89.0" layoutY="227.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="93.0" text="Load" />
            <Button layoutX="89.0" layoutY="301.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="93.0" text="Exit" />
         </children>
      </AnchorPane>
      <MenuBar layoutY="2.0" prefHeight="30.0" prefWidth="450.0">
        <menus>
          <Menu mnemonicParsing="false" text="Game">
            <items>
              <MenuItem mnemonicParsing="false" text="New" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Load" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="About">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
    </children>
</AnchorPane>
